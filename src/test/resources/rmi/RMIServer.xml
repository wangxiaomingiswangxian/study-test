<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
<!--服务端-->
    <bean id="helloRMIServiceImpl" class="study.rmi.app.HelloRMIServiceImpl"/>
    <bean id="myRMI" class="org.springframework.remoting.rmi.RmiServiceExporter">
        <property name="service" ref="helloRMIServiceImpl"/>
        <property name="serviceName" value="helloRMI"/>
        <property name="serviceInterface" value="study.rmi.client.HelloRMIService"/>
        <property name="registryPort" value="8090"/>
    </bean>
</beans>